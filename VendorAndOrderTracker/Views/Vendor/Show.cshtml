@{
  Layout = "_Layout";
}
@using VendorAndOrderTracker.Models;
<h3>Here are all orders:</h3>

<ol>
  @foreach (var order in @Model["order"])  
  {
    <li class="center"><h4><a href='/vendors/@Model["vendor"].Id/order/@order.Id'>@order.Title</a></h4></li> 
  }
</ol>
<p>
  <h3><a href='/vendors/@Model["vendor"].Id/order/new'><button class="btn btn-success">Add order</button></a></h3>
  <h3><a href='/'><button class="btn btn-success">Back</button></a></h3>
</p>

