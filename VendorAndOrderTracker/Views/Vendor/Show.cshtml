@{
  Layout = "_Layout";
}
@using VendorAndOrderTracker.Models;
<h3>Here are all orders:</h3>

<ol>
  @foreach (var order in @Model["order"])  
  {
    <li><a href='/vendors/@Model["vendor"].Id/order/@order.Id'>@order.Title</a></li> 
      
      
  }
</ol>

<p><a href='/vendors/@Model["vendor"].Id/order/new'>Add order</a></p>